									"table": "Course"
								},
								"destination": {
									"table": "Course"
								}
							},
							{
								"source": {
									"table": "SubTask"
								},
								"destination": {
									"table": "SubTask"
								}
							},
							{
								"source": {
									"table": "SubTaskLog"
								},
								"destination": {
									"table": "SubTaskLog"
								}
							},
							{
